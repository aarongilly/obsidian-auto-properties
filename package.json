{
	"name": "obsidian-auto-properties-plugin",
	"version": "1.0.0",
	"description": "This is a plugin for Obsidian (https://obsidian.md) to autofill properties based on note content and rules.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "property", "formulas", "plugin"],
	"author": "",
	"license": "MIT",
    "fundingUrl": "https://www.buymeacoffee.com/aarongilly",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
